<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jared.addingactivity">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Group.GroupAddActivity"></activity>
        <activity android:name=".Group.GroupEditActivity"></activity>
        <activity android:name=".Solo.SoloAddActivity"></activity>
        <activity android:name=".Solo.SoloEditActivity"></activity>
        <activity android:name=".SelectionAddActivity"></activity>
    </application>

</manifest>